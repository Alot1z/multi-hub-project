{
  "targetRepository": "https://github.com/Alot1z/ai-models",
  "branch": "main",
  "path": "/",
  "deploymentStrategy": "total-override",
  "netlifyConfig": {
    "siteId": "alot1z-ai-models",
    "buildCommand": "npm run build",
    "publishDirectory": "dist",
    "environmentVariables": {
      "NODE_ENV": "production",
      "VITE_API_BASE_URL": "https://alot1z-ai-models.netlify.app",
      "VITE_PLATFORM_BASE_URL": "https://alot1z.github.io",
      "VITE_ENABLE_LOCAL_INFERENCE": "true",
      "VITE_ENABLE_CUSTOM_QODO_GEN": "true",
      "VITE_FREE_TIER_OPTIMIZED": "true",
      "VITE_MAX_CONCURRENT_MODELS": "1",
      "VITE_ENABLE_MODEL_CACHING": "true"
    }
  },
  "gitMcpConfig": {
    "enableAutoCommit": true,
    "enableAutoDeployment": true,
    "commitMessage": "Auto-update AI Models from multi-hub-project",
    "updatePlatformTxt": true,
    "enableModelSync": true
  },
  "freetierOptimizations": {
    "enableBundleOptimization": true,
    "enableCodeSplitting": true,
    "enableLazyLoading": true,
    "maxBundleSize": "3MB",
    "enableServiceWorker": true,
    "enableOfflineMode": true,
    "enableModelCompression": true
  },
  "aiSpecificConfig": {
    "supportedModels": [
      "mistral-7b",
      "tinyllama", 
      "phi-2",
      "codellama"
    ],
    "enableLocalInference": true,
    "enableCustomQodoGen": true,
    "enableModelEnsemble": true,
    "maxModelSize": "4GB",
    "enableModelQuantization": true,
    "inferenceConfig": {
      "maxTokens": 4096,
      "temperature": 0.7,
      "topP": 0.9,
      "topK": 40,
      "enableStreaming": true
    }
  },
  "customQodoGenConfig": {
    "enableMultiModel": true,
    "enableAutoCodeGeneration": true,
    "enablePlatformIntegration": true,
    "outputQuality": "premium",
    "enable20xBetterResults": true
  },
  "securityConfig": {
    "enableCSP": true,
    "allowedOrigins": [
      "https://alot1z.github.io",
      "https://alot1z-hub-ui.netlify.app",
      "https://alot1z-ipa-builder.netlify.app",
      "https://alot1z-printer-builder.netlify.app",
      "https://alot1z-game-builder.netlify.app"
    ],
    "enableCORS": true,
    "enableSecurityHeaders": true,
    "enableModelValidation": true
  },
  "metadata": {
    "projectType": "ai-models",
    "description": "AI model router and prompt engine with Custom Qodo Gen",
    "lastUpdated": "2024-01-01T00:00:00Z",
    "autoGenerated": false,
    "version": "1.0.0",
    "features": [
      "Local Inference",
      "Custom Qodo Gen",
      "Model Management", 
      "20x Better Results",
      "No Rate Limits"
    ]
  }
}