{
  "targetRepository": "https://github.com/Alot1z/game-builder",
  "branch": "main",
  "path": "/",
  "deploymentStrategy": "total-override",
  "netlifyConfig": {
    "siteId": "game-build",
    "buildCommand": "npm run build",
    "publishDirectory": "dist",
    "environmentVariables": {
      "NODE_ENV": "production",
      "VITE_API_BASE_URL": "https://game-build.netlify.app",
      "VITE_PLATFORM_BASE_URL": "https://alot1z.github.io",
      "VITE_ENABLE_UNITY": "true",
      "VITE_ENABLE_MULTIPLAYER": "true",
      "VITE_FREE_TIER_OPTIMIZED": "true",
      "VITE_MAX_CONCURRENT_GAMES": "3"
    }
  },
  "gitMcpConfig": {
    "enableAutoCommit": true,
    "enableAutoDeployment": true,
    "commitMessage": "Auto-update Game Builder from multi-hub-project",
    "updatePlatformTxt": true
  },
  "freetierOptimizations": {
    "enableBundleOptimization": true,
    "enableCodeSplitting": true,
    "enableLazyLoading": true,
    "maxBundleSize": "4MB",
    "enableServiceWorker": true,
    "enableOfflineMode": true,
    "enableAssetCaching": true
  },
  "gameSpecificConfig": {
    "enableUnityIntegration": true,
    "enableMultiplayer": true,
    "enableAssetManagement": true,
    "enablePublishing": true,
    "supportedPlatforms": [
      "iOS",
      "Android",
      "WebGL",
      "Windows",
      "macOS"
    ],
    "gameEngines": [
      "Unity",
      "Unreal",
      "Godot",
      "Custom"
    ],
    "buildTargets": {
      "mobile": ["iOS", "Android"],
      "web": ["WebGL"],
      "desktop": ["Windows", "macOS", "Linux"]
    }
  },
  "securityConfig": {
    "enableCSP": true,
    "allowedOrigins": [
      "https://alot1z.github.io",
      "https://hub-uii.netlify.app",
      "https://game-build.netlify.app"
    ],
    "enableCORS": true,
    "enableSecurityHeaders": true,
    "enableAssetValidation": true
  },
  "metadata": {
    "projectType": "game-builder",
    "description": "Complete game development platform with Unity integration",
    "lastUpdated": "2024-01-01T00:00:00Z",
    "autoGenerated": false,
    "version": "1.0.0",
    "features": [
      "Unity Integration",
      "Asset Management",
      "Multiplayer Support",
      "Cross-platform Publishing",
      "Game Templates",
      "Performance Optimization"
    ]
  }
}