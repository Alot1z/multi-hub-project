{
  "targetRepository": "https://github.com/Alot1z/ipa-builder",
  "branch": "main",
  "path": "/",
  "deploymentStrategy": "total-override",
  "netlifyConfig": {
    "siteId": "alot1z-ipa-builder",
    "buildCommand": "npm run build",
    "publishDirectory": "dist",
    "environmentVariables": {
      "NODE_ENV": "production",
      "VITE_API_BASE_URL": "https://alot1z-ipa-builder.netlify.app",
      "VITE_PLATFORM_BASE_URL": "https://alot1z.github.io",
      "VITE_ENABLE_TROLLSTORE": "true",
      "VITE_ENABLE_CODE_SIGNING": "true",
      "VITE_FREE_TIER_OPTIMIZED": "true",
      "VITE_MAX_IPA_SIZE": "100MB"
    }
  },
  "gitMcpConfig": {
    "enableAutoCommit": true,
    "enableAutoDeployment": true,
    "commitMessage": "Auto-update IPA Builder from multi-hub-project",
    "updatePlatformTxt": true
  },
  "freetierOptimizations": {
    "enableBundleOptimization": true,
    "enableCodeSplitting": true,
    "enableLazyLoading": true,
    "maxBundleSize": "2MB",
    "enableServiceWorker": false,
    "enableOfflineMode": true
  },
  "iosSpecificConfig": {
    "enableTrollStore": true,
    "enableCodeSigning": true,
    "supportedIOSVersions": ["13.0", "14.0", "15.0", "16.0", "17.0"],
    "defaultEntitlements": [
      "com.apple.developer.icloud-container-identifiers",
      "com.apple.developer.ubiquity-kvstore-identifier"
    ],
    "buildTools": {
      "theos": "latest",
      "ldid": "2.1.5",
      "dpkg": "latest"
    }
  },
  "securityConfig": {
    "enableCSP": true,
    "allowedOrigins": [
      "https://alot1z.github.io",
      "https://alot1z-hub-ui.netlify.app"
    ],
    "enableCORS": true,
    "enableSecurityHeaders": true,
    "enableCodeSigningValidation": true
  },
  "metadata": {
    "projectType": "ipa-builder",
    "description": "iOS IPA builder with TrollStore support",
    "lastUpdated": "2024-01-01T00:00:00Z",
    "autoGenerated": false,
    "version": "1.0.0",
    "features": ["TrollStore", "Code Signing", "iOS Building", "TestFlight"]
  }
}