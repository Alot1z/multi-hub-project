{
  "name": "multi-hub-project",
  "version": "1.0.0",
  "description": "Enterprise-grade Multi-Hub platform ecosystem with AI ensemble system",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:hub-ui\" \"npm run dev:ipa\" \"npm run dev:printer\" \"npm run dev:game\" \"npm run dev:ai\"",
    "build": "npm run build:all",
    "build:all": "concurrently \"npm run build:hub-ui\" \"npm run build:ipa\" \"npm run build:printer\" \"npm run build:game\" \"npm run build:ai\"",
    "dev:hub-ui": "cd hub-ui && npm run dev",
    "dev:ipa": "cd ipa-builder && npm run dev",
    "dev:printer": "cd printer-builder && npm run dev",
    "dev:game": "cd game-builder && npm run dev",
    "dev:ai": "cd ai-models && npm run dev",
    "build:hub-ui": "cd hub-ui && npm run build",
    "build:ipa": "cd ipa-builder && npm run build",
    "build:printer": "cd printer-builder && npm run build",
    "build:game": "cd game-builder && npm run build",
    "build:ai": "cd ai-models && npm run build",
    "test": "npm run test:all",
    "test:all": "concurrently \"npm run test:hub-ui\" \"npm run test:ipa\" \"npm run test:printer\" \"npm run test:game\" \"npm run test:ai\"",
    "test:hub-ui": "cd hub-ui && npm test",
    "test:ipa": "cd ipa-builder && npm test",
    "test:printer": "cd printer-builder && npm test",
    "test:game": "cd game-builder && npm test",
    "test:ai": "cd ai-models && npm test",
    "lint": "npm run lint:all",
    "lint:all": "concurrently \"npm run lint:hub-ui\" \"npm run lint:ipa\" \"npm run lint:printer\" \"npm run lint:game\" \"npm run lint:ai\"",
    "lint:hub-ui": "cd hub-ui && npm run lint",
    "lint:ipa": "cd ipa-builder && npm run lint",
    "lint:printer": "cd printer-builder && npm run lint",
    "lint:game": "cd game-builder && npm run lint",
    "lint:ai": "cd ai-models && npm run lint",
    "deploy": "npm run deploy:all",
    "deploy:all": "concurrently \"npm run deploy:hub-ui\" \"npm run deploy:ipa\" \"npm run deploy:printer\" \"npm run deploy:game\" \"npm run deploy:ai\"",
    "deploy:hub-ui": "cd hub-ui && npm run deploy",
    "deploy:ipa": "cd ipa-builder && npm run deploy",
    "deploy:printer": "cd printer-builder && npm run deploy",
    "deploy:game": "cd game-builder && npm run deploy",
    "deploy:ai": "cd ai-models && npm run deploy"
  },
  "workspaces": [
    "hub-ui",
    "ipa-builder",
    "printer-builder", 
    "game-builder",
    "ai-models",
    "auth",
    "shared",
    "api",
    "bolt-new"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alot1z/multi-hub-project.git"
  },
  "keywords": [
    "multi-hub",
    "ai-ensemble",
    "ios-builder",
    "3d-printer",
    "game-builder",
    "netlify",
    "typescript",
    "react"
  ],
  "author": "Alot1z",
  "license": "MIT"
}
