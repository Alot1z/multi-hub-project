{
  "targetRepository": "https://github.com/Alot1z/printer-builder",
  "branch": "main",
  "path": "/",
  "deploymentStrategy": "total-override",
  "netlifyConfig": {
    "siteId": "printer-builder",
    "buildCommand": "npm run build",
    "publishDirectory": "dist",
    "environmentVariables": {
      "NODE_ENV": "production",
      "VITE_API_BASE_URL": "https://printer-builder.netlify.app",
      "VITE_PLATFORM_BASE_URL": "https://alot1z.github.io",
      "VITE_ENABLE_OPENSCAD": "true",
      "VITE_ENABLE_STL_EXPORT": "true",
      "VITE_FREE_TIER_OPTIMIZED": "true",
      "VITE_MAX_MODEL_SIZE": "50MB"
    }
  },
  "gitMcpConfig": {
    "enableAutoCommit": true,
    "enableAutoDeployment": true,
    "commitMessage": "Auto-update Printer Builder from multi-hub-project",
    "updatePlatformTxt": true
  },
  "freetierOptimizations": {
    "enableBundleOptimization": true,
    "enableCodeSplitting": true,
    "enableLazyLoading": true,
    "maxBundleSize": "3MB",
    "enableServiceWorker": true,
    "enableOfflineMode": true,
    "enableModelCaching": true
  },
  "printerSpecificConfig": {
    "enableOpenSCAD": true,
    "enableSTLExport": true,
    "enableParametricDesign": true,
    "enableMaterialCalculator": true,
    "supportedFormats": [
      "STL",
      "OBJ",
      "3MF",
      "PLY",
      "SCAD"
    ],
    "printerProfiles": [
      "Ender 3",
      "Prusa i3",
      "Ultimaker",
      "Custom"
    ],
    "materials": [
      "PLA",
      "ABS",
      "PETG",
      "TPU",
      "Wood",
      "Metal"
    ]
  },
  "securityConfig": {
    "enableCSP": true,
    "allowedOrigins": [
      "https://alot1z.github.io",
      "https://hub-uii.netlify.app",
      "https://printer-builder.netlify.app"
    ],
    "enableCORS": true,
    "enableSecurityHeaders": true,
    "enableFileValidation": true
  },
  "metadata": {
    "projectType": "printer-builder",
    "description": "3D model generator with OpenSCAD and STL export",
    "lastUpdated": "2024-01-01T00:00:00Z",
    "autoGenerated": false,
    "version": "1.0.0",
    "features": [
      "OpenSCAD Integration",
      "STL Export",
      "Parametric Design",
      "Material Calculator",
      "3D Preview",
      "Print Optimization"
    ]
  }
}